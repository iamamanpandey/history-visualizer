{
    "manifest_version": 3,
    "name": "Chrome History Visualizer",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "action": {
    "default_title": "Open History Analysis"
  },
  "background": {
    "service_worker": "background.js"
  },
    "permissions": ["history","tabs"],
    "host_permissions": ["*://*/*"]
  }